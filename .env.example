# .env.example
MONGODB_URI="mongodb+srv://<user>:<password>@yourcluster.mongodb.net/"
MONGODB_DB="vidseek_db"
MONGODB_COLLECTION="transcripts"

# Your Google AI Studio API Key for Gemini
GOOGLE_API_KEY="your_google_api_key_here"

# Set to "true" to force using the local sentence-transformer model
FORCE_LOCAL_EMBEDDINGS="false"